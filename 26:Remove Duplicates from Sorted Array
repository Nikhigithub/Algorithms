class Solution {
    public int removeDuplicates(int[] nums) {
      int result = 1;
      int end=nums.length;  
      int count =0;
      int j=1; 
        int i=0;
      while(j<end){
       if(nums[i]!=nums[j] ){
           nums[i+1]=nums[j];
           i++;
           result++;
         }  
          j++;
      } 
        
      return result;  
    }
}
